{% for type, messagesOfType in messages %}
    {% for message in messagesOfType %}
        <div class="alert alert-{{ type }} alert-dismissible fade show" role="alert">
            {{ message|raw }}
            <button
                type="button"
                class="btn-close"
                data-bs-dismiss="alert"
                aria-label="Close"
            ></button>
        </div>
    {% endfor %}
{% endfor %}
